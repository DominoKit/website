<module rename-to="app">
  <inherits name="org.dominokit.domino.gwt.Core"/>
  <inherits name="org.dominokit.domino.api.Domino"/>
  <inherits name="org.dominokit.website.layout.LayoutClientModule"/>
  <inherits name="org.dominokit.website.layout.LayoutUIClientModule"/>
  <inherits name="com.google.gwt.user.User"/>
  <inherits name="org.dominokit.domino.logger.Logging"/>
  <source path=""/>
  <entry-point class="org.dominokit.website.AppClientModule"/>
  <set-property name="gwt.logging.logLevel" value="INFO"/>
  <set-property name="gwt.logging.enabled" value="TRUE"/>
  <set-property name="gwt.logging.consoleHandler" value="ENABLED"/>
  <set-property name="compiler.useSourceMaps" value="true"/>
  <set-configuration-property name="includeSourceMapUrl" value="static/gwt/extra/app/symbolMaps/__HASH___sourceMap__FRAGMENT__.json"/>
  <collapse-all-properties/>
</module>